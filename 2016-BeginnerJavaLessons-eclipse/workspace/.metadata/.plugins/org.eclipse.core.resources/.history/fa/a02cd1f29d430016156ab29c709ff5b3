package ders4;

import java.util.Scanner;

public class Ders4 {
	
	public static void main(String[] args)
	{
 /* çoklu yorum
  * satýrý için 
  * bu kod kullanýlýr
  * 
  */
		Scanner tara = new Scanner(System.in);
		
		int taranacak;
		String yazi;
				
		taranacak = tara.nextInt();
		System.out.println(taranacak);
		
		
		
		System.out.println("Yazý Yazýnýz:");
		
		
		yazi = tara.nextLine();
		
		System.out.println("Bunu Yazdýn: " + yazi);
		
		
  }
}

